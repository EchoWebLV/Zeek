{
  "name": "zeke-privacy-bot",
  "version": "2.0.0",
  "description": "Privacy-focused X bot and shielded blog service using Gemini AI and Zcash",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts --local",
    "generate": "tsx src/index.ts --local",
    "blog": "tsx src/blog.ts",
    "blog:watch": "tsx src/blog.ts --watch",
    "blog:demo": "tsx src/blog.ts --demo",
    "blog:live": "tsx src/blog.ts --live",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "privacy",
    "bot",
    "gemini",
    "imagen",
    "twitter",
    "x",
    "zcash",
    "shielded",
    "blog"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.0.0",
    "@grpc/grpc-js": "^1.12.0",
    "@grpc/proto-loader": "^0.7.13",
    "@noble/ciphers": "^2.0.1",
    "@noble/curves": "^1.7.0",
    "@noble/hashes": "^1.6.0",
    "bs58check": "^4.0.0",
    "dotenv": "^16.4.5",
    "sharp": "^0.34.5",
    "twitter-api-v2": "^1.17.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
