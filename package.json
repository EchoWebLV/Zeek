  "description": "Privacy-focused X bot using Gemini 3.0 for text and Imagen for images",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts --local",
    "generate": "tsx src/index.ts --local",
    "typecheck": "tsc --noEmit"
